#include <stdio.h>

int main() {
     int num[10];
     int i;
     int even=0;
     int odd=0;
     
     
     printf("enter 10 numbers\n");
     for(i=1;i<10;i++)
     
    {
        printf("numbers %d", i);
        scanf("%d ", &num[i]);
    }
    for(i=0;i<10;i++)
    {
        if(num[i]%2==0)
        {
            even++;
        }
        else {
            odd++;
        }
    }
        printf("Positive numbers: %d\n", even);
    printf("Negative numbers: %d\n", odd);


    
    return 0;
}
