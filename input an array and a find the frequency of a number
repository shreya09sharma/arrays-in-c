// Online C compiler to run C program online
#include <stdio.h>

int main() {
     int num[100];
     int i;
     int count=0;
     int number;
     int size;
      printf("Enter the size of the array: ");
    scanf("%d", &size);
     
     printf("enter the numbers\n");
     for(i=1;i<size;i++)
     
    {
        printf("numbers %d", i);
        scanf("%d ", &num[i]);
    }
    printf("enter the number to find frequency\n");
    scanf("%d", &number);
    for(int i=0;i<10;i++)
    {
        if(num[i]==number)
        count++;
    }
    printf("The number %d appears %d time(s) in the array.\n", number, count);
    
    return 0;
}
