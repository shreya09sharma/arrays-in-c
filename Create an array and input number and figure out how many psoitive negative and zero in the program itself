#include <stdio.h>

int main() {
     int num[10];
     int i;
     int positive=0;
     int negative=0;
     int zero=0;
     
     printf("enter 10 numbers\n");
     for(i=1;i<10;i++)
     
    {
        printf("numbers %d", i);
        scanf("%d", &num[i]);
    }
    for(i=0;i<10;i++)
    {
        if(num[i]>0)
        {
            positive++;
        }
        else if(num[i] < 0) {
            negative++;
        }
        else {
            zero++;
        }
    }
        printf("Positive numbers: %d\n", positive);
    printf("Negative numbers: %d\n", negative);
    printf("Zeros: %d\n", zero);

    
    return 0;
}
